se, "is_req_repeatable": false, "repeat_limit_attempts": null, "repeat_limit_credits": null, "catalog_levels": [], "skills": [], "attributes": [{"name": "discussion intensive", "id": 2, "type": "attribute"}, {"name": "diversity", "id": 3, "type": "attribute"}, {"name": "ethics", "id": 5, "type": "attribute"}, {"name": "for non-majors", "id": 6, "type": "attribute"}, {"name": "reading intensive", "id": 13, "type": "attribute"}, {"name": "sustainability/environment", "id": 16, "type": "attribute"}, {"name": "intercultural & global lrng", "id": 22, "type": "attribute"}, {"name": "equity & inclusion", "id": 23, "type": "attribute"}], "custom_fields": {"goals": "Fall 2023 understand how literature and the forces of social change interact, will help students considering an English major learn how it approaches social issues", "key_topics": "Fall 2023: protest, visual arts, literature, Victorian, romantic, inequality, social justice, empire, race, gender", "prerequisite_knowledge": "Fall 2023: none", "assessment_structure": "Oral presentations, short writing assignments, pariticpation", "relevance": "Fall 2023: will teach non-majors the landscape of literary and cultural history, will prepare those who want to become English majors for what literary study is like", "learning_resources": "Fall 2023: 1 or 2 novels to purchase; other readings available on Canvas or Persuall", "extra_time_commitment": null}, "admin_context": {"suggested_by_advisor": null, "counts_for": []}, "success": true}