], "offering_tags": [{"name": "specific_sem", "type": "INFO", "sem": 1}], "website": null, "instructors": [], "is_repeatable": false, "is_req_repeatable": false, "repeat_limit_attempts": null, "repeat_limit_credits": null, "catalog_levels": [], "skills": [{"name": "Print Making", "slug": "print-making", "id": 1466}], "attributes": [{"name": "ethics", "id": 5, "type": "attribute"}, {"name": "for non-majors", "id": 6, "type": "attribute"}, {"name": "writing intensive", "id": 18, "type": "attribute"}, {"name": "communication & presentation", "id": 19, "type": "attribute"}, {"name": "diversity", "id": 3, "type": "attribute"}, {"name": "equity & inclusion", "id": 23, "type": "attribute"}], "custom_fields": {"goals": "Improved written communication skills, of course, but also greater confidence in their writing and a more discerning eye for evaluating news coverage.", "key_topics": "Journalistic writing styles and discussions on how news media cover current events", "prerequisite_knowledge": "Basic English grammar skills", "assessment_structure": "Seven graded assignments and periodic in-class exercises. There is no final.", "relevance": "Journalistic writing, at its core, emphasizes clear, concise and accurate written communication -- skills that will help students in whatever their chosen field turns out to be.", "learning_resources": "Copy of required text will be on reserve at Hunt Library.", "extra_time_commitment": null}, "admin_context": {"suggested_by_advisor": null, "counts_for": []}, "success": true}