ctors": [], "is_repeatable": false, "is_req_repeatable": false, "repeat_limit_attempts": null, "repeat_limit_credits": null, "catalog_levels": [], "skills": [], "attributes": [{"name": "discussion intensive", "id": 2, "type": "attribute"}, {"name": "diversity", "id": 3, "type": "attribute"}, {"name": "ethics", "id": 5, "type": "attribute"}, {"name": "interdisciplinary", "id": 10, "type": "attribute"}, {"name": "reading intensive", "id": 13, "type": "attribute"}, {"name": "team project", "id": 17, "type": "attribute"}, {"name": "writing intensive", "id": 18, "type": "attribute"}, {"name": "communication & presentation", "id": 19, "type": "attribute"}, {"name": "intercultural & global lrng", "id": 22, "type": "attribute"}, {"name": "equity & inclusion", "id": 23, "type": "attribute"}], "custom_fields": {"goals": "Exposure to a variety of research methods, expanded knowledge of academic fields and disciplines, written communication and analytical skills.", "key_topics": "Literary History and Criticism, History of Social Movements, Writing, Critical Thiking Skills, Discussions of Race, Gender and Sexuality", "prerequisite_knowledge": "Prior course work in literary studies.", "assessment_structure": "Group and individual projects, presentations and writing assignments.", "relevance": null, "learning_resources": "Readings and visual materials will be available in pdf form or through a link on Canvas. They will also be required to purchase books.", "extra_time_commitment": null}, "admin_context": {"suggested_by_advisor": null, "counts_for": []}, "success": true}