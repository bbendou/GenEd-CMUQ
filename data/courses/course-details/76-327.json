", "student_sets": [{"id": 896, "name": "undergraduate"}], "offering_tags": [{"name": "intermittent", "type": "INFO"}], "website": null, "instructors": [], "is_repeatable": false, "is_req_repeatable": false, "repeat_limit_attempts": null, "repeat_limit_credits": null, "catalog_levels": [], "skills": [{"name": "Fiction", "slug": "fiction", "id": 1455}], "attributes": [{"name": "discussion intensive", "id": 2, "type": "attribute"}, {"name": "diversity", "id": 3, "type": "attribute"}, {"name": "ethics", "id": 5, "type": "attribute"}, {"name": "for non-majors", "id": 6, "type": "attribute"}, {"name": "interdisciplinary", "id": 10, "type": "attribute"}, {"name": "communication & presentation", "id": 19, "type": "attribute"}, {"name": "intercultural & global lrng", "id": 22, "type": "attribute"}, {"name": "equity & inclusion", "id": 23, "type": "attribute"}], "custom_fields": {"goals": "To learn concrete strategies for change and rhetorical strategies for arguing in favor of that change", "key_topics": "race, gender, disability, language, intersectionality, professional communication", "prerequisite_knowledge": "A willingness to read texts from a variety of fields and disciplines", "assessment_structure": "take home essay exams,informal presentations, a final project on a topic of the student's choosing, short, informal responses", "relevance": "This class focuses on specific strategies that can be implemented in workplaces, schools, and other institutions to make their practices more equitable. The content should be relevant to students in rhetoric, professional writing, gender studies, and those who want to make change in their institutions.", "learning_resources": "course website, PDFs of course readings, podcasts and videos", "extra_time_commitment": "The time required to read and complete assignments."}, "admin_context": {"suggested_by_advisor": null, "counts_for": []}, "success": true}